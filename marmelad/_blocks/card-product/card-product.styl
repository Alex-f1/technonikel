.card-product
  .property
    .name-parameters
      font-size: 16px
      margin-bottom: 16px

  &__container
    position: relative

    @media (min-width: 960px)
      display: flex

  &__icon
    position: absolute
    top: 40px
    left: 0
    flex: 0 0 45px
    width: 45px
    max-width: 45px
    height: 45px
    background-color: #FFF
    border-radius: 10px
    display: flex
    justify-content: center
    align-items: center
    transition: background-color 0.5s
    z-index: 10

    @media (max-width: 959px)
      top: 10px
      left: 10px

    @media (min-width: 1920px)
      top: 40px
      left: -110px

    &:hover
      span
        opacity: 1
        visibility: visible

    span
      font-size: 14px
      color: #000
      font-style: normal
      font-weight: 300
      line-height: 1.3
      position: absolute
      white-space: nowrap
      top: 0
      left: 125%
      width: 175px
      padding: 3px 8px
      background-color: #FFF
      box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.25)
      border-radius: 5px
      display: block
      opacity: 0
      visibility: hidden
      transition: opacity 0.3s
      z-index: 5

  &__images
    @media (max-width: 959px)
      // display: flex
      // align-items: center
      // overflow-x: auto
      margin-bottom: 30px
      width: 100%
      max-width: 100%

    @media (min-width: 960px)
      flex: 0 0 380px
      width: 380px
      max-width: 380px
      margin-right: 25px

    @media (min-width: 1280px)
      flex: 0 0 585px
      width: 585px
      max-width: 585px
      margin-right: 45px

    @media (min-width: 1600px)
      flex: 0 0 810px
      width: 810px
      max-width: 810px
      margin-right: 98px

    p
      text-align: center
      color: #000
      margin-top: -50px !important
      transform: translateX(-10%)

      @media (max-width: 959px)
        // display: none
        transform: translate(0, 35px)
        margin-bottom 60px

  &__images-slider
    margin-bottom: 60px

    @media (max-width: 959px)
      // flex: 0 0 350px
      // width: 350px
      // max-width: 350px
      margin-left: 0
      // margin-right: 20px
      margin-bottom: 20px
      border-radius: 10px

    // @media (max-width: 413px)
    //   flex: 0 0 280px
    //   width: 280px
    //   max-width: 280px

    @media (min-width: 960px)
      border-radius: 0px 30px 30px 0px
      margin-left: -50px

    @media (min-width: 1280px)
      margin-left: -50px

    @media (min-width: 1600px)
      margin-left: -150px

    @media (min-width: 1921px)
      border-radius: 30px

    & ^[0]
      &__img
        width: 100% !important
        margin-left: 0 !important
        margin-bottom: 0 !important

    .swiper-button-prev, .swiper-button-next
      width: 60px
      height: 60px
      top: auto
      bottom: 20px
      margin: 0
      background-color: #fff
      border-radius: 50%
      transition: background-color 0.3s

      @media (max-width: 1279px)
        width: 40px
        height: 40px

      &:hover
        background-color: #4FA600

        &:after
          background-image: url("data:image/svg+xml,%3Csvg width='14' height='24' viewBox='0 0 14 24' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M0.851562 1.09082L11.7607 11.9999L0.851562 22.909' stroke='%23fff' stroke-width='2'/%3E%3C/svg%3E%0A")

      &:after
        content: ''
        width: 14px
        height: 24px
        background-image: url("data:image/svg+xml,%3Csvg width='14' height='24' viewBox='0 0 14 24' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M0.851562 1.09082L11.7607 11.9999L0.851562 22.909' stroke='black' stroke-width='2'/%3E%3C/svg%3E%0A")
        background-repeat: no-repeat
        background-position: 0 0
        background-size: contain

        @media (max-width: 1279px)
          width: 8px
          height: 14px

    .swiper-button-prev
      left: auto
      right: 90px
      transform: rotate(180deg)

      @media (max-width: 1279px)
        right: 60px

    .swiper-button-next
      right: 20px

      @media (max-width: 1279px)
        right: 10px

  &__img
    margin-bottom: 60px
    background-color: #F1F1F1
    overflow: hidden
    cursor: pointer
    border-radius: 10px

    @media (max-width: 959px)
      // flex: 0 0 350px
      // width: 350px
      // max-width: 350px
      height: 456px
      // margin-right: 20px
      margin-bottom: 20px
      
    @media (max-width: 639px)
      height 256px

    @media (min-width: 960px)
      border-radius: 0px 30px 30px 0px
      margin-left: -50px
      height: 500px

    @media (min-width: 1280px)
      margin-left: -50px
      height: 600px

    @media (min-width: 1600px)
      margin-left: -150px
      height: 700px

    @media (min-width: 1921px)
      border-radius: 30px

    img
      vertical-align: top
      width: 100%
      height: 100%
      object-fit: cover

  &__total-price
    display: flex
    align-items: center
    justify-content: space-between
    margin-bottom: 40px

    @media (max-width: 639px)
      flex-direction: column

  &__price
    font-size: 36px
    color: #000
    font-weight: 300
    line-height: 1.2
    letter-spacing: 0.8px
    margin-right: 25px
    flex: 0 0 auto

    @media (max-width: 639px)
      margin-right: 0
      margin-bottom: 25px

  &__button-buy
    @media (max-width: 639px)
      width: 100%

    @media (max-width: 1280px)
      width: 250px

    @media (min-width: 1600px)
      width: 352px

    &.button-default
      padding-top: 21px
      padding-bottom: 21px

  &__links
    display: flex
    align-items: center
    padding-bottom: 40px

    @media (max-width: 959px)
      flex-direction: column

    a
      font-size: 16px
      color: #000
      font-weight: 300
      line-height: 1.1
      letter-spacing: 0.8px
      text-decoration: none
      border: 1px solid #4FA600
      border-radius: 10px
      width: 50%
      height: 80px
      display: flex
      align-items: center
      justify-content: center
      padding: 30px 20px
      transition: background-color 0.3s, border-color 0.3s, color 0.3s

      @media (max-width: 639px)
        height: 60px
        padding-top: 15px
        padding-bottom: 15px

      @media (max-width: 959px)
        width: 100%
        border-radius: 10px !important
        border: 1px solid #4FA600 !important
        margin-bottom: 10px

      &:before
        content: ''
        margin-right: 8px
        background-repeat: no-repeat
        background-position: 0 0
        background-size: contain
        transition: background 0.3s

      &:hover
        color: #fff
        background-color: #4FA600
        border-color: transparent

  &__link-location
    border-radius: 10px 0 0 10px !important
    border-right: none !important

    &:before
      flex: 15px
      width: 15px
      max-width: 15px
      height: 21px
